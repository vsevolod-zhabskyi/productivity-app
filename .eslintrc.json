{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 80,
        "ignoreUrls": true
      }
    ],
    "no-duplicate-imports": "error",
    "camelcase": "error",
    "eqeqeq": "error",
    "no-unused-vars": "warn"
  }
}
